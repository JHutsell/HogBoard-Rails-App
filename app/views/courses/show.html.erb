<h1><%= @course.name %></h1>
<h3>Taught by: <%= link_to @course.professor.name, professor_path(@course.professor) %></h3>

<h4>Class Description: </h4>
<p><%= @course.description %></p>
<p>Class Roster: </p>
<ul>
    <% @students.each do |student| %>
        <li>
            <%= link_to student.name, student_path(student) %>
        </li>
    <% end %>
</ul>